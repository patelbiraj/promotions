<?xml version="1.0" encoding="UTF-8"?>
<web-ext xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://websphere.ibm.com/xml/ns/javaee"
    xsi:schemaLocation="http://websphere.ibm.com/xml/ns/javaee http://websphere.ibm.com/xml/ns/javaee/ibm-web-ext_1_1.xsd"
    version="1.1">
  <jsp-attribute name="jdkSourceLevel" value="16" />
  <reload-interval value="3"/>
  <context-root uri="secondchance" />
  <resource-ref name="Inet2DataResource" connection-management-policy="AGGRESSIVE"/>
  <enable-directory-browsing value="false"/>
  <enable-file-serving value="true"/>
  <pre-compile-jsps value="true"/>
  <enable-reloading value="true"/>
  <enable-serving-servlets-by-class-name value="false"/>
</web-ext>